{
  "$jason": {
    "head": {
      "title": "Test",
      "description": "Test",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://giffynet.github.io/jasonelle/test.json",
            "method": "get"
          },
          "success": {
            "type": "$render"
          },
          "error": {
            "type": "$util.toast",
            "options": {
              "type": "warning",
              "text": "Unable to  fetch batch summary from server. Please check your internet connection."
            }
          }
        },
        "$foreground": {
          "type": "$reload"
        }
      },
      "templates": {
        "body": {
          "background": "#f3f3f3",
          "sections": [{
            "items": [{
              "type": "vertical",
              "style": {
                "align": "center",
                "padding": "20",
                "distribution": "equalspacing"
              },
              "components": [{
                "type": "label",
                "text": "{{$jason.subsonicresponse.status}}"
              },{
                "type": "button",
                "text": "Back",
                "style": {
                  "background": "#d9d9d9",
                  "color": "#000000",
                  "height": "80",
                  "size": "24"
                },
                "action": {
                  "type": "$back"
                }
              }]
            }]
          }]
        }
      }
    }
  }
}
